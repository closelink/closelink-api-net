<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>CloselinkAPI</id>

        <title>CloselinkAPI</title>
        <description>.NET Wrapper for Closelink API</description>
        <summary>A very fast, thin, and intuitive .NET Wrapper for the Closelink API.</summary>

        <version>1.0.0</version>
        <authors>Tobias Schumacher</authors>
        <owners>Closelink GmbH</owners>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <developmentDependency>false</developmentDependency>
        <copyright>Copyright Â© 2020</copyright>

        <PackageProjectUrl>https://github.com/closelink/closelink-api-net</PackageProjectUrl>
        <RepositoryUrl>https://github.com/closelink/closelink-api-net</RepositoryUrl>
        <RepositoryType>Git</RepositoryType>

        <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>

        <dependencies>
            <dependency id="JsonSubTypes" version="1.2.0" />
            <dependency id="Microsoft.CSharp" version="4.7.0" />
            <dependency id="Newtonsoft.Json" version="12.0.3" />
            <dependency id="RestSharp" version="106.0.0" />
            <dependency id="System.ComponentModel.Annotations" version="4.7.0" />
        </dependencies>
    </metadata>
    <files>
        <file src="..\README.md" target="" />
        <file src="..\docs\**\*.*" target="docs" />
    </files>
</package>
